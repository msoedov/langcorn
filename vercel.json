{
    "devCommand": "uvicorn examples.app:vercel_app --host 0.0.0.0 --port 3000",
    "builds": [
        {
            "src": "examples/app.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "examples/app.py"
        }
    ]
}
